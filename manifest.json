{
  "manifest_version": 2,
  "name": "Extension Manager",
  "description": "Disable other extensions in new tab popup",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "management"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
